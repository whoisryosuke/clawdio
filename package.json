{
  "name": "clawdio-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "modules/*/pkg",
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace clawdio-examples dev",
    "build": "yarn build && yarn build:modules",
    "build:modules": "node scripts/build.js",
    "build:library": "yarn workspace clawdio build"
  }
}
